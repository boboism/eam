= simple_form_for(@asset_transfer, html: {class: "well form-horizontal"}) do |f|

  = f.error_notification
  = display_base_errors @asset_transfer

  .form-inputs
    = f.input :effective_date, as: :string, input_html: { class: "input-block-level", :"data-behavior" => "datepicker" }
    = render 'asset_transfer_asset', f: f

  .form-actions
    = f.button :submit, t('helpers.links.submit'), class: 'btn-primary'
